    "recompose": "^0.30.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.2",
    "react-dom": "^16.8.0 || ^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
