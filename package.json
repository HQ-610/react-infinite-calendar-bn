{
  "name": "@appannie/react-infinite-calendar",
  "version": "3.3.5",
  "description": "Infinite scrolling date-picker built with React, with localization, themes, keyboard support, and more.",
  "main": "lib",
  "module": "es",
  "umd:main": "umd",
  "files": [
    "es",
    "lib",
    "umd",
    "styles.css"
  ],
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production rollup -c",
    "build:watch": "NODE_ENV=production rollup -c -w",
    "prepare": "husky install && yarn build",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "esw --ext src/**/*.js src/index.js --color",
    "lint:fix": "yarn lint --fix",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
